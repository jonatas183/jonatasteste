#!/bin/bash
clear

VermelhoAll='\e[01;37;41m'
NADA='\033[0m'
CINZA='\e[02;37m'
DESTACAR='\e[01;37m'
RED='\033[1;31m'
GREEN='\033[1;32m'
YELLOW='\033[1;33m'
SCOLOR='\033[0m'
SEMCOR='\033[8m'
AZUL='\033[36m'
PURPULA='\033[35m'
AMARELO='\033[33m'
MenuPrincipal(){
clear
echo -e '\n'
echo -e "         ${PURPULA}================================="
echo -e "                ${GREEN}MB FREE @JonatasMaciel    "
echo "        ==================================="
echo -e "               ${AZUL}Qual brinquedo para hoje"
echo "        ===================================" 
echo -e "            ${DESTACAR}[ 1 ] - ${YELLOW}Ganhar MB"
echo -e "            ${DESTACAR}[ 0 ] - ${RED}Sair, tenho medo"${AZUL}
echo "        ==================================="
echo

echo 
    read -n1 -p "Escolhe uma carta " App
    case $App in  
    1)Menu5;;
    0) clear ; exit;;
    *) echo ; MenuPrincipal;;
    esac
     }
Menu5(){
rm -f chavedeacesso.txt
clear
echo -e '\n'
echo -e "        ${PURPULA}======================================"
echo -e "                  ${GREEN}MB FREE @JonatasMaciel    "
echo -e "       ==========================================" 
echo -e "                ${AMARELO}Escolha uma opção:"
echo -e "       ${PURPULA}============================================="
echo -e "              ${DESTACAR}[ 1 ] - ${AMARELO}Meu accessPass"
echo -e "              ${DESTACAR}[ 2 ] - ${AMARELO}Usar accessPass da crush"
echo -e "              ${DESTACAR}[ 3 ] - ${AMARELO}Usar accessPass memorizados"
echo -e "              ${DESTACAR}[ 4 ] - ${AZUL}Como pegar accessPass"
echo -e "              ${DESTACAR}[ 0 ] - ${RED}Voltar${NADA}"
echo -e "       ${AZUL}==========================================="
echo 
echo
read -n1 -p "Escolhe uma carta " TokensNews01
case $TokensNews01 in
1)ProcurarAPass ;;
2)chavecustom ;;
3)PassSalvos0;;
4)ComoPegarOPass;;
0)MenuPrincipal;;
*)Menu5;;
esac
}

ProcurarAPass(){
clear
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'

echo -e "============================================${NADA}"

echo -e "${LETRAPRETA}                  AGUARDE                  ${NADA}"

echo -e "============================================${NADA}"

echo -e "${DESTACAR}Sem WI-FI, sem VPN, use dados "
echo -e "móveis limpo, para gerar o ${GREEN}PASS${DESTACAR} e então a"
echo -e "diversão começa"

echo

echo -e "Digite ${GREEN}Y${DESTACAR} para continuar ${NADA}"

 read -n1 Option005
 case $Option005 in
 Y);;
 y);;
 *)Menu5;;
 esac
 
 

AccessPass=$(curl -s -d "pageId=660224&version=2" i.vivo.ddivulga.com/i/gp | cut -d'"' -f4)
echo -e "$AccessPass" >> chavedeacesso.txt
clear
echo -e "============================================${NADA}"

echo -e "${LETRAPRETA}                  AGUARDE                  ${NADA}"

echo -e "============================================${NADA}"
echo -e "${DESTACAR}${AZUL}estamos verificando sua linha${NADA}"
sleep 4
Buscartokens
}

chavecustom(){
clear
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'
echo -e "============================================${NADA}"

echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"

echo -e "============================================${NADA}"
read -p $'\e[01;33mCole o accessPass: \033[1;32m' AccessPass
echo
SalvarPass
}

SalvarPass(){
clear
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'
echo -e "${NADA}============================================${NADA}"

echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"

echo -e "============================================${NADA}"
read -p $'\e[01;37mmemorizar accessPass? [Y/n] ' AccessPassSave
echo
case $AccessPassSave in
n)Buscartokens;;
N)Buscartokens;;
Y)read -p $'\e[01;37mpõe um nome legal pro accessPass: ' PassName ; echo -e " ${GREEN} $PassName${NADA}-$AccessPass" >> TokensSave.txt ; Buscartokens;;
y)read -p $'\e[01;37mpõe um nome legal pro accessPass: ' PassName ; echo -e "${GREEN} $PassName${NADA}-$AccessPass" >> TokensSave.txt ; Buscartokens;;
*)SalvarPass;;
esac
}

