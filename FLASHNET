NewTokensRev(){
clear
AZUL='\e[01;36;m'
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'

echo -e "============================================${NADA}"
echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"
echo -e "============================================${NADA}"

TokensNovos00=$(cat advidcriar.txt |  grep -c ,)
clear
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'

echo -e "============================================${NADA}"
echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"
echo -e "============================================${NADA}"

case $TokensNovos00 in
1) echo -e "${GREEN}1 ads novo ${NADA}" ;;
2) echo -e "${GREEN}2 ads novos ${NADA}" ;;
3) echo -e "${GREEN}3 ads novos ${NADA}" ;;
4) echo -e "${GREEN}4 ads novos ${NADA}" ;;
5) echo -e "${GREEN}5 ads novos ${NADA}" ;;
6) echo -e "${GREEN}6 ads novos ${NADA}" ;;
7) echo -e "${GREEN}7 ads novos ${NADA}" ;;
8) echo -e "${GREEN}8 ads novos ${NADA}" ;;
9) echo -e "${GREEN}9 ads novos ${NADA}" ;;
10) echo -e "${GREEN}10 ads novos ${NADA}" ;;
*) echo -e "${RED}nenhum ads novo${NADA}" ;;
esac

NumeroDeTelefone=$(cat tokencreator2.txt | grep -o msisdn.................................... | cut -d '"' -f3 | grep -n 55 | grep 1: | cut -d':' -f2)

echo
echo -e "${CINZA}Número da vítima:${NADA} ${GREEN}$NumeroDeTelefone${NADA}"
echo -e "${CINZA}total de ads encontrados:${NADA} ${GREEN}$TokensNovos00/10"
